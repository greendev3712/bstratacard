        <ServiceInstall Name="IntellaUpdate"
                  DisplayName="IntellaUpdate"
                  Description="Updater service used for Intellasoft line of software"
                  ErrorControl="ignore"
                  Type="ownProcess" 
                  Vital="yes"
                  Start="auto"
                  Account="LocalSystem"
                  Interactive="no" />
        <ServiceControl Id="ServiceControl_IntellaUpdate"
                  Name="IntellaUpdate"
                  Start="install"
                  Stop="both"
                  Remove="uninstall"
                  Wait="yes" />
      </Component>
      
